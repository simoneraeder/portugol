programa {
  funcao inicio() {

  inteiro numero
  inteiro sucessor
  inteiro antecessor
  

   escreva("Digite um número:", "\n")
   leia(numero)

   antecessor = numero - 1
   sucessor = numero + 1

   escreva("O antecessor de", numero, "é", antecessor, "\n")
   escreva("O sucessor de", numero, "é", sucessor ,"\n")


  }
}
