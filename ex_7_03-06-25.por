programa {
  funcao inicio() {
     inteiro dividendo
    inteiro divisor
    inteiro quociente
    inteiro resto
    

    escreva("Digite o dividendo (número inteiro):")
    leia(dividendo)

    escreva("Digite o divisor (número inteiro):")
    leia(divisor)

quociente = dividendo / divisor
resto = dividendo % divisor


    escreva( "Quociente:", quociente ,  "\n")
    escreva("Resto: ", resto, "\n")


  }
}
